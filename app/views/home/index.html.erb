<div class="row-fluid">
  <div class="span12">
    <div class="span5"></div>
    <div class="span7">
      <span id="search_box">
        <input type="text" value="" name="search" 
        id="search_options_container" placeholder="Find user by name">
        <input type="image" src="/assets/store_search_btn.gif" 
        class="search_auction_items">
      </span>
      <h3>All Results </h3>
      <div class="user_list <%='LeftCol' if @users.present? %>">
        <%= render :partial => '/users/user_list', :collection => @users %>
        <div class="pagination_margin"><%= paginate @users %></div>
      </div>
    </div>
  </div>
</div>      
